//Evaluates the factorials of the integers from 1 to 5
#include<stdio.h>
#include<conio.h>
#include<math.h> 
int main()
{
    int i,n;
 	unsigned int fact = 1;
	printf("Enter number for calculation of factorial: ");
    scanf("%d",&n);
	    for(i=1;i<=n;i++)
	    {
	    	fact = fact*i;
	    
}
	printf("Factorial is: %d\n",fact);
	return 0;
}
